{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/custom/api/(.*)",
      "dest": "dist/src/app.js"
    }
  ],
  "headers": [
    {
      "source": "/custom/api/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    },
    {
      "source": "/node-express-ts-typeorm-planetscale-redis-auth.vercel.app/custom/api/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}
